{
  "hosting": {
    "public": "./dist",
    "redirects": [{
      "source" : "/app.bundle.js",
      "destination" : "/app.bundle.js.gz",
      "type" : 301
    },{
      "source" : "/vendor.bundle.js",
      "destination" : "/vendor.bundle.js.gz",
      "type" : 301
    },{
      "source" : "/polyfill.bundle.js",
      "destination" : "/polyfill.bundle.js.gz",
      "type" : 301
    }],
    "headers": [{
      "source" : "**/*.@(gz)",
      "headers" : [{
        "key" : "Content-Encoding",
        "value" : "gzip"
      }]
    }]
  }
}
