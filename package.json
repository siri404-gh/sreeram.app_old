{
  "name": "sreeram.app",
  "version": "1.0.0",
  "description": "Sreeram Padmanabhan's personal website",
  "main": "src/web/index.js",
  "scripts": {
    "build": "webpack --env=prod --config=webpack/webpack.config.js",
    "build:dev": "webpack --env=dev --config=webpack/webpack.config.js",
    "build:firebase": "webpack --env=firebase --config=webpack/webpack.config.js",
    "build:devserver": "webpack-dev-server --open --env=dev --config=webpack/webpack.config.js",
    "test": "karma start",
    "lint:local": "eslint .",
    "lint:html": "eslint . -o=./dist/eslint/index.html -f=html",
    "commit": "git-cz",
    "docker": "docker-compose -f docker-compose-web.yml up",
    "lighthouse": "lighthouse",
    "lighthouse:pr": "lighthouse-ci",
    "firebase:deploy": "firebase deploy --token $FIREBASE_TOKEN",
    "clean": "rimraf dist reports",
    "styleguide": "npx styleguidist server",
    "styleguide:eject": "npx styleguidist build",
    "test:browserstack": "karma start karma.browserstack.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sreerampr/sreeram.app.git"
  },
  "author": "Sreeram Padmanabhan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sreerampr/sreeram.app/issues"
  },
  "homepage": "https://github.com/sreerampr/sreeram.app#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "codacy-coverage": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "codecov": "^3.0.4",
    "commitizen": "^2.10.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.16.3",
    "firebase-tools": "^4.0.2",
    "heroku": "^7.7.8",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.13",
    "jasmine-core": "^3.1.0",
    "karma": "^3.0.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "lighthouse": "^3.0.3",
    "lighthouse-ci": "git+https://github.com/ebidel/lighthouse-ci.git",
    "node-notifier-cli": "^1.1.2",
    "path": "^0.12.7",
    "react-hot-loader": "^4.3.4",
    "react-styleguidist": "^7.1.3",
    "rimraf": "^2.6.2",
    "sonarqube-scanner": "^2.1.0",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.4",
    "webpack-visualizer-plugin": "^0.1.11",
    "workbox-webpack-plugin": "^3.4.1"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^2.0.1",
    "airbrake-js": "^1.4.4",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "logrocket": "^0.6.16",
    "logrocket-react": "^2.0.0",
    "promise-polyfill": "8.1.0",
    "raven-js": "^3.26.4",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "request": "^2.87.0",
    "save": "^2.3.2",
    "whatwg-fetch": "^2.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/notify -t 'sreeram.app' -m 'Are you being a responsible commitizen?'",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-commit": "./node_modules/.bin/notify -t 'sreeram.app' -m 'Commit Successful' -s Glass"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": "8.11.3",
    "npm": "6.3.0"
  }
}
